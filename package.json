{
  "name": "zaloapi-monorepo",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "build": "pnpm --filter './packages/**' build",
    "bumpp": "bumpp packages/*/package.json --no-commit --no-tag --no-push",
    "release": "pnpm bumpp && pnpm -r publish --access public"
  },
  "devDependencies": {
    "@daopk/eslint-config-ts": "^0.4.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.45.0"
  }
}
