    "@daopk/eslint-config-node": "1.0.0-beta.8",
    "@daopk/eslint-config-stylistic": "1.0.0-beta.8",
    "@daopk/eslint-config-typescript": "1.0.0-beta.8",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "eslint-flat-config-viewer": "^0.1.1"
  }
}
